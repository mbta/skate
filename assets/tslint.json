{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-hooks",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "react-hooks-nesting": "error",
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "require-const-for-all-caps",
      "allow-pascal-case"
    ]
  },
  "rulesDirectory": []
}
